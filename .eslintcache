ow start=\"totalDeductions\" end=\"incomeTax\" {...incomeTaxInStyle} />\r\n      <Xarrow start=\"totalDeductions\" end=\"ni\" {...niInStyle} />\r\n      <Xarrow start=\"yearlyNet\" end=\"monthlyNet\" {...yearlyNetOutStyle} />\r\n      <Xarrow start=\"monthlyNet\" end=\"weeklyNet\" {...incomingStyle} />\r\n\r\n      <h4>Income Flowchart</h4>\r\n      <div className=\"data\">\r\n        <div className=\"featureNumber\" id=\"annualGross\">\r\n          <h5>Annual (Gross) salary</h5>\r\n          <div className=\"item\">\r\n            <span className=\"incoming\">\r\n              <span className=\"symbol\">{currencySymbol} </span>\r\n            </span>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"featureNumber\" id=\"taxfree\">\r\n          <h5>Tax Free Allowance</h5>\r\n          <div className=\"item\">\r\n            <span className=\"incoming\">\r\n              <span className=\"symbol\">{currencySymbol} </span>\r\n            </span>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"featureNumber\" id=\"taxable\">\r\n          <h5>Taxable</h5>\r\n          <div className=\"item\">\r\n            <span className=\"incoming\">\r\n              <span className=\"symbol\">{currencySymbol} </span>\r\n            </span>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"featureNumber\" id=\"ni\">\r\n          <h5>National Insurance</h5>\r\n          <div className=\"item\">\r\n            <span className=\"outgoing\">\r\n              <span className=\"symbol\">{currencySymbol} </span>\r\n            </span>\r\n          </div>\r\n        </div>\r\n        <div className=\"featureNumber\" id=\"incomeTax\">\r\n          <h5>Income Tax</h5>\r\n          <div className=\"item\">\r\n            <span className=\"outgoing\">\r\n              <span className=\"symbol\">{currencySymbol} </span>\r\n            </span>\r\n          </div>\r\n        </div>\r\n        <div className=\"featureNumber\" id=\"totalDe